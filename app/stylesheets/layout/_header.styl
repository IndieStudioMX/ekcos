		left -7px
	.navbar-products
		position absolute
		width 100%
		height 3.3rem
		left 15rem
		background $navbar-submenu
		top 3.9rem 
		li 
			float right
			font-size 1.25rem
			margin-left 0
			&:hover 
				background url('../images/shadow-sub.png')
				background-size 100% 100%
				background-position 0 -1px 
				background-repeat no-repeat 
				height 33px
			&:first-child 
				width 34rem
				background url('../images/docs-bg.svg') no-repeat
				height 63px
				background-position 29px -23px
				background-size 100% 133%
				&:hover 
					background url('../images/docs-bg-hover.svg') no-repeat
					width 34rem
					background-position 33px -25px
					background-size 94% 134%
					height 60px
					height: 6.12rem;
				& > a 
					color $text-color-light
					margin-left 9rem
					
			&:nth-child(2)
				margin-right -5.8rem
				padding .5rem 3.2rem 0 2rem
				transform skew(29deg)
				transform skew(29deg)
				-webkit-transform: skew(29deg)
				-moz-transform skew(29deg)
				-ms-transform skew(29deg)
				& > a 
					margin-top -6px
					margin-left 0
					-webkit-transform skew(-29deg)
					-moz-transform skew(-29deg)
					-ms-transform skew(-29deg)
					transform skew(-29deg)
				&:hover 
					background-color $navbar-submenu_shadow
					background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -moz-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -ms-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: linear-gradient(to top right, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					margin-right -6rem
					padding-right 3.22rem
					margin-left .2rem
					height: 3.15rem;
					-webkit-transform skew(29deg)
					-moz-transform scale(1.1) skew(29deg)
					-ms-transform scale(1.1) skew(29deg)
					transform scale(1.1) skew(29deg)
					-webkit-transition .1s all ease-in
					-moz-transition .1s all ease-in
					-ms-transition .1s all ease-in
					transition .1s all ease-in
			&:nth-child(3) 
				padding-left .5rem
				padding-right .6rem
				margin-right .1rem
				position relative
				left 0
				-webkit-transform skew(29deg)
				-moz-transform skew(29deg)
				-ms-transform skew(29deg)
				transform skew(29deg)
				&:hover 
					background-color $navbar-submenu_shadow
					background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -moz-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -ms-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: linear-gradient(to top right, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					left 0
					padding-right 0.59rem; 
					margin-right 0
					margin-left .15rem 
					height: 3.15rem;
					-webkit-transform skew(29deg)
					-moz-transform scale(1.1) skew(29deg)
					-ms-transform scale(1.1) skew(29deg)
					transform scale(1.1) skew(29deg)
					-webkit-transition .1s all ease-in
					-moz-transition .1s all ease-in
					-ms-transition .1s all ease-in
					transition .1s all ease-in
				& > a 
					-webkit-transform: skew(-29deg);
					-moz-transform: skew(-29deg);
					-ms-transform: skew(-29deg);
			&:last-child
				padding-left 1rem
				padding-right 1.15rem
				margin-right: 0;
				position relative
				left 0
				transform: skew(29deg);
				-webkit-transform: skew(29deg);
				-moz-transform: skew(29deg);
				-ms-transform: skew(29deg);
				&:hover 
					background-color $navbar-submenu_shadow
					background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -moz-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: -ms-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					background-image: linear-gradient(to top right, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
					margin-right 0
					padding-right 1.04rem
					left 0
					height: 3.15rem;
					-webkit-transform skew(29deg)
					-moz-transform scale(1.1) skew(29deg)
					-ms-transform scale(1.1) skew(29deg)
					transform scale(1.1) skew(29deg)
					-webkit-transition .1s all ease-in
					-moz-transition .1s all ease-in
					-ms-transition .1s all ease-in
					transition .1s all ease-in
				& > a 
					-webkit-transform: skew(-29deg);
					-moz-transform: skew(-29deg);
					-ms-transform: skew(-29deg);
			& > a 
				color $text-color
	.nav.navbar-nav.navbar-right 
		margin-right 3.5rem !important

	.navbar-default .navbar-nav li 
		& > a 
			font-size 1.25rem
			color $text-color
		& > .item-menu
			transition-menu .1s ease-in
		& > .item-menu:hover 
			border-bottom 3px solid $font-active-color
			transition-menu .2s ease-in
		&:nth-child(2), &:nth-child(3), &:nth-child(4), &:nth-child(5)
			margin-left -0.1rem
		&:last-child 
			position absolute
			left 39.5rem
			top -2px
			width 337px
			background url('../images/view-cart.svg') no-repeat 100% 100%
			background-position -7px -14px
			&:hover 
				background url('../images/view-cart_hover.svg') no-repeat 100% 100%
				background-position -7px -14px
			& > a 
				color $text-color
				margin-left  7rem
				text-transform uppercase
				font-size 1rem
				height 50px
				line-height 2.25rem
				&:hover 
					color $text-color
				& > img
					width 25%
					position absolute 
					top -.6rem
					left 5.9rem
	.collapse 
		display block
		visibility initial
