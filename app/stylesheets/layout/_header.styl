li
	margin-left 1em

.nav > li > a 
	padding 10px 1px 0 0
	margin 0 8px
	margin 0 14.5px

.nav > li > a:hover, .nav > li > a:focus
	background-color transparent

.navbar.navbar-default.navbar-fixed-top 
	background $menu-light
	border-color transparent
	padding 22px 0
	height 83px

.navbar-default .navbar-nav li 
	& > a 
		font-size 1.25rem
		color $text-color
		
	& > .item-menu
		transition-menu .1s ease-in
	& > .item-menu:hover 
		border-bottom 3px solid $font-active-color
		transition-menu .2s ease-in
	&:nth-child(2), &:nth-child(3), &:nth-child(4), &:nth-child(5)
		margin-left -0.1rem
	&:last-child 
		position absolute
		left 39.5rem
		top -2px
		width 337px
		background url('../images/view-cart.svg') no-repeat 100% 100%
		background-position -7px -14px
		&:hover 
			background url('../images/view-cart_hover.svg') no-repeat 100% 100%
			background-position -7px -14px
		& > a 
			color $text-color
			margin-left  7rem
			text-transform uppercase
			font-size 1rem
			height 50px
			line-height 2.25rem
			&:hover 
				color $text-color
			& > img
				width 25%
				position absolute 
				top -.6rem
				left 5.9rem

.nav.navbar-nav.navbar-right 
	margin-right 3.5rem;

.navbar-header 
	margin-top -22px

.brand 
	display block
	height auto
	position absolute
	top -10px
	left -7px
	z-index 2
	img 
		max-width 100%
		width 450px
		
.main-menu 
	top -1rem
	left -14.5rem
	position relative
	
.nav-line 
	position absolute
	height 4px
	background $navbar-line
	width 100%
	border-top 0
	left 60rem
	top 1.5rem

.navbar-products-mobile 
	position absolute
	width 100%
	height 3.3rem
	left 0rem
	background $navbar-submenu
	top 6rem 
	font-weight bold
	li 
		float right
		font-size 1.25rem
		&:first-child 
			margin-left 3%
			& > a 
				color #444
		&:nth-child(2) 
			margin-left 5%
			& > a 
				color #444
		&:last-child 
			margin-left -6%
			& > a 
				color #444
.navbar-products
	position absolute
	width 100%
	height 3.3rem
	left 15rem
	background $navbar-submenu
	top 3.9rem 
	font-weight bold
	li 
		float right
		font-size 1.25rem
		margin-left 0
		&:hover 
			background url('../images/shadow-sub.png')
			background-size 100% 100%
			background-position 0 -1px 
			background-repeat no-repeat 
			height 33px
		&:first-child 
			width 34rem
			background url('../images/docs-bg.svg') no-repeat
			height 63px
			background-position 29px -23px
			background-size 100% 133%
			&:hover 
				background url('../images/docs-bg-hover.svg') no-repeat
				width 34rem
				background-position 31px -25px
				background-size 95% 134%
				height 60px
				
			& > a 
				color $text-color-light
				margin-left 9rem
				
		&:nth-child(2)
			margin-right -5.8rem
			padding .5rem 3.2rem 0 2rem
			transform skew(29deg)
			transform skew(29deg)
			-webkit-transform: skew(29deg)
			-moz-transform skew(29deg)
			-ms-transform skew(29deg)
			& > a 
				margin-top -6px
				margin-left 0
				-webkit-transform skew(-29deg)
				-moz-transform skew(-29deg)
				-ms-transform skew(-29deg)
				transform skew(-29deg)
			&:hover 
				background-color $navbar-submenu_shadow
				background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -moz-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -ms-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: linear-gradient(to top right, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				margin-right -6.1rem
				padding-right 3.22rem
				margin-left .2rem
				height: 3.15rem;
				-webkit-transform skew(29deg)
				-moz-transform skew(29deg)
				-ms-transform skew(29deg)
				transform skew(29deg)
				-webkit-transition .1s all ease-in
				-moz-transition .1s all ease-in
				-ms-transition .1s all ease-in
				transition .1s all ease-in
				
				
		&:nth-child(3) 
			padding-left .5rem
			padding-right .6rem
			margin-right .1rem
			position relative
			left 0
			-webkit-transform skew(29deg)
			-moz-transform skew(29deg)
			-ms-transform skew(29deg)
			transform skew(29deg)
			&:hover 
				background-color $navbar-submenu_shadow
				background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -moz-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -ms-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: linear-gradient(to top right, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				left 0
				padding-right 0.59rem;
				margin-right 0
				margin-left .15rem 
				height: 3.15rem;
				
			& > a 
				-webkit-transform: skew(-29deg);
				-moz-transform: skew(-29deg);
				-ms-transform: skew(-29deg);
		&:last-child
			padding-left 1rem
			padding-right 1.15rem
			margin-right: 0;
			position relative
			left 0
			-webkit-transform skew(29deg)
			-moz-transform skew(29deg)
			-ms-transform skew(29deg)
			transform skew(29deg)
			-webkit-transition .1s all ease-in
			-moz-transition .1s all ease-in
			-ms-transition .1s all ease-in
			transition .1s all ease-in
			&:hover 
				background-color $navbar-submenu_shadow
				background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -moz-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: -ms-linear-gradient(bottom left, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				background-image: linear-gradient(to top right, rgba(255,255,255,0) 0%, rgba(255,255,255,.4) 100%)
				margin-right 0
				padding-right 1.04rem
				transition .5s all ease-in
				left 0
				height: 3.15rem;
			& > a 
				-webkit-transform: skew(-29deg);
				-moz-transform: skew(-29deg);
				-ms-transform: skew(-29deg);
		& > a 
			color $text-color
			
