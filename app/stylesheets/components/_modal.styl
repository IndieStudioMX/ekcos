		width 100% !important
		margin-left 0 !important
		height 210rem !important
	.modal-title 
		text-align center
	.docs-img, .img-responsive
		margin 0 auto
		
	.power-m
		display inline-block 
		vertical-align middle 
		width 48%
		margin .5rem 0 
		padding 0
	
	.ek-m, .ek-it, .ez-d, .ez-img, .tru-d
		display inline-block 
		vertical-align middle 
		width 48% 
		margin .5rem 0
		padding 0
		margin .5rem auto .5rem auto
	.ez-img, .tru-img 
