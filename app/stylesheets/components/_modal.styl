.modal-title 
	text-transform uppercase
	color #000
	font-weight bold 
	position relative 
	margin 0 0 1rem 0
	left 0 
	font-size 1.4rem 
	padding .1rem 0
	letter-spacing 3px
	text-align center
	background $font-active-color
	span 
		text-transform none 
		
.row-power 
	display inline-block 
	vertical-align top 
	width 45%
	border 1px solid $font-active-color 
	margin 0 2.3%
	padding 0 0 1.3rem 0
	
.row-power-gallery
	width 95.4%
	margin 6rem auto 1rem auto
	border 1px solid $font-active-color  
	height 35rem
	
.covers 
	width 98% !important
	margin-left 1% !important 
	
.power-d a 
	margin 0 auto
	display block
	
.coverflow-p
	width 100%
	
.flipster 
	width 98% !important
	
.power-d01
	background: url("../images/power-com01.png");
	background-repeat no-repeat 
	background-size 100%
	width 100% 
	height 84px 
	display block 
	margin 0 auto
	transition .4s ease-in 
	-webkit-transition .4s ease-in 
	-moz-transition .4s ease-in 
	-ms-transition .4s ease-in 
	&:hover 
		content ""
		background url('../images/power-com_hov.png')
		background-repeat: no-repeat;
		background-size 100%
		transition .2s ease-in 
		-webkit-transition .2s ease-in 
		-moz-transition .2s ease-in 
		-ms-transition .2s ease-in 
		-webkit-transform scale(1.1)
		-moz-transform scale(1.1)
		-ms-transform scale(1.1)
		transform scale(1.1)
		display: block;
		max-width: 100%
		height: 84px;
		margin: 0 auto;
		
